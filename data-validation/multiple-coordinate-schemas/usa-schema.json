{
  "title": "USA Location Data",
  "description": "A schema that matches any objects with longitude and latitude coordinates near the USA",
  "required": [
    "latitude",
    "longitude"
  ],
  "type": "object",
  "properties": {
    "latitude": {
      "type": "number",
      "minimum": 20,
      "maximum": 50
    },
    "longitude": {
      "type": "number",
      "minimum": -130,
      "maximum": -60
    }
  }
}